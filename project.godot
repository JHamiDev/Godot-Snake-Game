;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Snake"
config/description="A snake clone/remake"
run/main_scene="res://Scenes/Level.tscn"
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://Sprites/SnakeSegment.png"
config/macos_native_icon="res://SnakeSegment32pix.icns"
config/windows_native_icon="res://SnakeSegmentIcon.ico"

[autoload]

GameManager="*res://Scenes/GameManager.tscn"

[display]

window/size/resizable=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://Font Data/default_env.tres"
